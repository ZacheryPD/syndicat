{
    "parserOptions": {
        "ecmaVersion": 8
    },
    "env": {
        "node": true,
        "mocha": true
    },
    "globals": {
      "l": false,
      "co": false,
      "console": false,
      "chai": false,
      "expect": false,
      "chaiAsPromised": false,
      "fs": false,
      "getTestDB": false,
      "createTestModel": false,
      "ModelUtils": false,
      "readonly": false
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-unused-vars": "off"
    }
}
